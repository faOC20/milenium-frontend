---
import Layout from "@layouts/Layout.astro";
import RevenueTable from "@components/RevenueTable.astro";
import {MonthSummary} from "@components/MonthSummary";
import type { number } from "astro:schema";

const {id} = Astro.params
---

<Layout style="">
    <div class="w-full h-full flex flex-col gap-[3rem] mb-8 pt-8 px-10">
        <RevenueTable year = {id || undefined}/>
        <MonthSummary client:only year = {id}/>
    </div>
</Layout>